<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="countDownTimerRootAnchorPane" xmlns="http://javafx.com/javafx/13.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <left>
            <Button fx:id="prevTimerButton" mnemonicParsing="false" onAction="#onTimerPrevButtonClicked" text="Previous" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="10.0" />
               </BorderPane.margin>
            </Button>
         </left>
         <right>
            <Button fx:id="nextTimerButton" mnemonicParsing="false" onAction="#onTimerNextButtonClicked" text="Next" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets right="10.0" />
               </BorderPane.margin>
            </Button>
         </right>
         <bottom>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="startTimerButton" alignment="CENTER" mnemonicParsing="false" onAction="#onTimerStartButtonClicked" text="Start">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="pauseTimerButton" alignment="CENTER" mnemonicParsing="false" onAction="#onTimerPauseButtonClicked" text="Pause">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="resetTimerButton" alignment="CENTER" mnemonicParsing="false" onAction="#onTimerResetButtonClicked" text="Reset">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="exitTimerButton" alignment="CENTER" mnemonicParsing="false" onAction="#onTimerExitButtonClicked" text="exit">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </bottom>
         <top>
            <VBox alignment="CENTER" prefHeight="59.0" prefWidth="560.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="workTitleLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="37.0" prefWidth="580.0" text="Title Label" textAlignment="JUSTIFY">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
                  <Label fx:id="numSetRemainingLabel" alignment="CENTER" prefHeight="16.0" prefWidth="263.0" text="Remaining Set Label" textAlignment="CENTER">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </top>
         <center>
            <AnchorPane prefHeight="196.0" prefWidth="400.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER" layoutX="62.0" layoutY="26.0" prefHeight="144.0" prefWidth="334.0">
                     <children>
                        <Label fx:id="hrsLabel" alignment="CENTER" prefHeight="176.0" prefWidth="81.0" text="Hrs">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                        <Label fx:id="sepColonHrsMinLabel" alignment="CENTER" prefHeight="144.0" prefWidth="13.0" text=":">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <Label fx:id="minLabel" alignment="CENTER" prefHeight="142.0" prefWidth="88.0" text="Min">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                        <Label fx:id="sepColonMinSecLabel" alignment="CENTER" prefHeight="138.0" prefWidth="11.0" text=":">
                           <HBox.margin>
                              <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                           </HBox.margin>
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <Label fx:id="secLabel" alignment="CENTER" prefHeight="170.0" prefWidth="86.0" text="Sec">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
